{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" },
    {
      "source": "/api/(.*)",
      "destination": "https://ttlab-be-tan.vercel.app/api/v1"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "methods": ["GET", "POST"],
      "dest": "https://ttlab-be-tan.vercel.app/api/v1"
    }
  ]
}
